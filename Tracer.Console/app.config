<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <system.diagnostics>
        <trace autoflush="true"/>
        <sharedListeners>
            <add name="Console" type="System.Diagnostics.ConsoleTraceListener"/>
        </sharedListeners>
        <sources>
            <source name="*" switchValue="All">
                <listeners>
                    <add name="Console"/>
                </listeners>
            </source>
        </sources>
    </system.diagnostics>
</configuration>
